{
    "presets": [
        [
            "env",
            {
                "modules": false
            }
        ],
        "stage-2",
        "es2015",
        "react"
    ],
    "plugins": [
        "transform-runtime",
        "transform-decorators-legacy",
        "transform-class-properties",
        "react-hot-loader/babel"
    ],
    "comments": false,
    "env": {
        "test": {
            "presets": [
                "env",
                "stage-2"
            ],
            "plugins": [
                "istanbul"
            ]
        }
    }
}